<form method="POST" action="/sequences/<%= @sequence.id %>">

  <ul>

    <li>
      <label for="name"> Sequence Name: </label>
      <input value = "<%= @sequence.name %>" type="text" name="name" id="name"/>
    </li>

    <li>
      <label for="total_time"> Total Time (in minutes): </label>
      <input value = "<%= @sequence.total_time %>" type="text" name="total_time" id="total_time"/>
    </li>

    <li>
      <label for="average_effort_level"> Average Effort Level: </label>
      <input value = "<%= @sequence.average_effort_level %>" type="average_effort_level" name="average_effort_level" id="average_effort_level"/>
    </li>

    <li>
      <label for="pose_id_1"> Pose: </label>
      <input value = "<%= @sequence.pose_id_1 %>" type="pose_id_1" name="pose_id_1" id="pose_id_1"/>
    </li>

    <li>
      <label for="pose_id_2"> Pose: </label>
      <input value = "<%= @sequence.pose_id_2 %>" type="pose_id_2" name="pose_id_2" id="pose_id_2"/>
    </li>

    <li>
      <input type="submit" name="Save"/>
    </li>

  </ul>
</form>